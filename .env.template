# Environment configuration for Teams-Azure DevOps MCP deployment
# These variables will be used by azd and the Bicep templates

# Azure subscription and location (deployment only)
AZURE_SUBSCRIPTION_ID=
AZURE_LOCATION=eastus

# Environment configuration (deployment only)
AZURE_ENV_NAME=teams-ado-mcp

# Service configuration (deployment only)
SERVICE_MCP_SERVER_IMAGE_NAME=
SERVICE_TEAMS_BOT_IMAGE_NAME=

# ⚠️  FOR LOCAL DEVELOPMENT: Use 'dotnet user-secrets set "AzureDevOps:PersonalAccessToken" "your-pat"'
# Azure DevOps configuration (set this before deployment)
AZURE_DEVOPS_PAT=

# Principal ID for Key Vault access (will be set by azd)
AZURE_PRINCIPAL_ID=

# Generated resource identifiers (set by deployment)
CONTAINER_APPS_ENVIRONMENT_NAME=
CONTAINER_REGISTRY_NAME=
CONTAINER_REGISTRY_LOGIN_SERVER=
KEY_VAULT_NAME=
KEY_VAULT_URI=
LOG_ANALYTICS_WORKSPACE_NAME=
LOG_ANALYTICS_WORKSPACE_ID=
APPLICATION_INSIGHTS_NAME=
APPLICATION_INSIGHTS_CONNECTION_STRING=
USER_ASSIGNED_IDENTITY_NAME=
USER_ASSIGNED_IDENTITY_ID=
USER_ASSIGNED_IDENTITY_CLIENT_ID=
MCP_SERVER_APP_NAME=
MCP_SERVER_APP_FQDN=
TEAMS_APP_NAME=
TEAMS_APP_FQDN=
RESOURCE_GROUP_NAME=
SUBSCRIPTION_ID=
